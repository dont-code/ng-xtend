{
  "name": "dont-code-workflow",
  "version": "0.6.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-deploy": "APP=dc-workflow && ng build $APP --configuration=$BUILD",
    "watch": "ng build --watch --configuration development",
    "test": "cd projects/dc-workflow && ../../node_modules/.bin/vitest && cd ../wfw-tester && ../../node_modules/.bin/vitest"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "repository": {
    "url": "https://github.com/dont-code/ng-xtend.git"
  },
  "publishConfig": {
    "directory": "dist/dc-workflow",
    "linkDirectory": true
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^20.3.9",
    "@angular/common": "^20.3.9",
    "@angular/compiler": "^20.3.9",
    "@angular/core": "^20.3.9",
    "@angular/forms": "^20.3.9",
    "@angular/platform-browser": "^20.3.9",
    "@angular/platform-browser-dynamic": "^20.3.9",
    "@angular/router": "^20.3.9",
    "@angular/cdk": "^20.2.4",
    "rxjs": "^7.8.2",
    "primeng": "^20.3.0",
    "@primeuix/themes": "^1.2.5",
    "primeicons": "^7.0.0",
    "tslib": "^2.8.1",
    "xt-type": "workspace:^",
    "xt-store": "workspace:^",
    "xt-components": "workspace:^"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^1.22.4",
    "@analogjs/vitest-angular": "^1.22.4",
    "@angular-devkit/build-angular": "^20.3.8",
    "@angular/build": "^20.3.8",
    "@angular/cli": "^20.3.8",
    "@angular/compiler-cli": "^20.3.9",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "ng-packagr": "^20.3.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-tsconfig-paths": "~5.1.4",
    "vitest": "^3.2.4"
  }
}
