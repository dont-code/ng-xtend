{
  "name": "xt-components",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build xt-components --configuration=development && ng build tester-app --configuration=development ",
    "build-deploy": "APP=xt-components && ng build $APP --configuration=$BUILD",
    "watch": "ng build --watch --configuration development",
    "test": "cd projects/xt-components && ../../node_modules/.bin/vitest && cd ../tester-app && ../../node_modules/.bin/vitest"
  },
  "publishConfig": {
    "directory": "dist/xt-components",
    "linkDirectory": true
  },

  "private": false,
  "dependencies": {
    "@angular/animations": "^20.2.3",
    "@angular/common": "^20.2.3",
    "@angular/compiler": "^20.2.3",
    "@angular/core": "^20.2.3",
    "@angular/forms": "^20.2.3",
    "@angular/platform-browser": "^20.2.3",
    "@angular/platform-browser-dynamic": "^20.2.3",
    "@angular/router": "^20.2.3",
    "@angular/cdk": "^20.2.4",
    "rxjs": "^7.8.2",
    "primeng": "^20.1.2",
    "xt-type": "workspace:^",
    "@primeuix/themes": "^1.2.3",
    "primeicons": "^7.0.0",
    "tslib": "^2.8.1",
    "@angular-architects/native-federation": "^20.1.6",
    "@softarc/native-federation-runtime": "^3.3.5"

  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.2.1",
    "@angular/cli": "^20.2.1",
    "@angular/compiler-cli": "^20.2.3",
    "ng-packagr": "^20.2.0",
    "typescript": "^5.9.2",
    "happy-dom": "^18.0.1",
    "@analogjs/vite-plugin-angular":"^1.20.1",
    "@analogjs/vitest-angular": "^1.20.1",
    "jsdom": "^26.1.0",
    "vite": "^7.1.6",
    "vitest": "^3.2.4",
    "vite-tsconfig-paths": "~5.1.4",
    "@primeuix/utils": "^0.6.1",
    "@primeuix/styles": "^1.2.3",
    "@primeuix/styled": "^0.7.2"
  }
}
