{
  "name": "xt-components",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build xt-components --configuration=development && ng build tester-app --configuration=development ",
    "build-deploy": "APP=xt-components && ng build $APP --configuration=$BUILD",
    "watch": "ng build --watch --configuration development",
    "test": "cd projects/xt-components && ../../node_modules/.bin/vitest && cd ../tester-app && ../../node_modules/.bin/vitest"
  },
  "publishConfig": {
    "directory": "dist/xt-components",
    "linkDirectory": true
  },

  "private": false,
  "dependencies": {
    "@angular/animations": "^19.2.10",
    "@angular/common": "^19.2.10",
    "@angular/compiler": "^19.2.10",
    "@angular/core": "^19.2.10",
    "@angular/forms": "^19.2.10",
    "@angular/platform-browser": "^19.2.10",
    "@angular/platform-browser-dynamic": "^19.2.10",
    "@angular/router": "^19.2.10",
    "@angular/cdk": "^19.2.15",
    "rxjs": "^7.8.2",
    "primeng": "^19.1.3",
    "xt-type": "workspace:^",
    "@primeng/themes": "^19.1.3",
    "@primeuix/utils": "^0.5.3",
    "@primeuix/styled": "^0.6.4",
    "primeicons": "^7.0.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.11",
    "@angular/cli": "^19.2.11",
    "@angular/compiler-cli": "^19.2.10",
    "ng-packagr": "^19.2.2",
    "typescript": "^5.7.3",
    "happy-dom": "^17.5.6",
    "@analogjs/vite-plugin-angular":"^1.17.1",
    "@analogjs/vitest-angular": "^1.17.1",
    "jsdom": "^26.1.0",
    "vite": "^6.3.5",
    "vitest": "^3.1.4",
    "vite-tsconfig-paths": "~5.1.4"

  }
}
