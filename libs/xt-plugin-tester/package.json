    "ng-packagr": "^19.2.0",
    "typescript": "^5.7.3",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.5.3",
    "@happy-dom/jest-environment": "^17.4.4",
    "happy-dom": "^17.4.4",
    "@angular-builders/jest": "^19.0.0"
  }
