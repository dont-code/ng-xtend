{
  "name": "xt-plugin-tester",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build plugin-tester --configuration=development",
    "build-deploy": "APP=plugin-tester && ng build $APP --configuration=$BUILD --output-path=\"../../dist/$STAGE/$APP\" --base-href=\"https://test.dont-code.net/apps/$STAGE/$APP/\"  && rm -f ../../dist/$STAGE/$APP/*.* && mv ../../dist/$STAGE/$APP/browser/* ../../dist/$STAGE/$APP",
    "watch": "ng build --watch --configuration development",
    "test": "cd projects/plugin-tester && ../../node_modules/.bin/jest"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^19.2.4",
    "@angular/common": "^19.2.4",
