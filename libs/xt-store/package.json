{
  "name": "xt-store",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-deploy": "APP=store && ng build $APP --configuration=$BUILD",
    "watch": "ng build --watch --configuration development",
    "test": "cd projects/store && ../../node_modules/.bin/vitest"
  },
  "publishConfig": {
    "directory": "dist/store",
    "linkDirectory": true
  },

  "private": false,
  "dependencies": {
    "@angular/animations": "^20.2.3",
    "@angular/common": "^20.2.3",
    "@angular/compiler": "^20.2.3",
    "@angular/core": "^20.2.3",
    "@angular/forms": "^20.2.3",
    "@angular/platform-browser": "^20.2.3",
    "@angular/platform-browser-dynamic": "^20.2.3",
    "@angular/router": "^20.2.3",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "@ngrx/signals": "^20.0.1",
    "xt-type": "workspace:^"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.2.1",
    "@angular/cli": "^20.2.1",
    "@angular/compiler-cli": "^20.2.3",
    "happy-dom": "^18.0.1",
    "ng-packagr": "^20.2.0",
    "typescript": "^5.9.2",
    "@analogjs/vite-plugin-angular":"^1.20.1",
    "@analogjs/vitest-angular": "^1.20.1",
    "jsdom": "^26.1.0",
    "vite": "^7.1.6",
    "vitest": "^3.2.4",
    "vite-tsconfig-paths": "~5.1.4"
  }
}
