{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "pnpm",
    "analytics": "8440f8d0-3f6c-4758-b71e-f9ee11217c7f",
    "cache": {
      "enabled": false
    }
  },
  "newProjectRoot": "projects",
  "projects": {
    "web": {
      "projectType": "library",
      "root": "projects/web",
      "sourceRoot": "projects/web/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/web/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/web/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/web/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
					"builder": "@analogjs/vitest-angular:test",
          "options": {
            "tsConfig": "projects/web/tsconfig.spec.json",
            "polyfills": [
            ]
          }
        }
      }
    },
    "web-plugin": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "css"
        }
      },
      "root": "projects/web-plugin",
      "sourceRoot": "projects/web-plugin/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-architects/native-federation:build",
          "options": {},
          "configurations": {
            "production": {
              "target": "web-plugin:esbuild:production"
            },
            "development": {
              "target": "web-plugin:esbuild:development",
              "dev": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-architects/native-federation:build",
          "options": {
            "target": "web-plugin:serve-original:development",
            "rebuildDelay": 0,
            "dev": true,
            "port": 0
          }
        },
        "esbuild": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/xt-web-plugin",
            "index": "projects/web-plugin/src/index.html",
            "browser": "projects/web-plugin/src/main.ts",
            "preserveSymlinks": true,
            "polyfills": [
              "es-module-shims"
            ],
            "tsConfig": "projects/web-plugin/tsconfig.app.json",
            "inlineStyleLanguage": "css",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/web-plugin/public"
              }
            ],
            "styles": [
              "projects/web-plugin/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1MB",
                  "maximumError": "2MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
					"defaultConfiguration": "development"
        },
				"serve-original": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "web-plugin:esbuild:production"
            },
            "development": {
              "buildTarget": "web-plugin:esbuild:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 4202
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@analogjs/vitest-angular:test",
          "options": {
            "polyfills": [],
            "tsConfig": "projects/web-plugin/tsconfig.spec.json",
            "inlineStyleLanguage": "css",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/web-plugin/public"
              }
            ],
            "styles": [
              "projects/web-plugin/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
