{
  "name": "xt-plugin-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build web",
    "build-deploy": "APP=web-plugin && MAIN_LIB=web && ng build $MAIN_LIB --configuration=$BUILD && ng build $APP --configuration=$BUILD --output-path=\"../../dist/$STAGE/$APP\" --base-href=\"https://test.dont-code.net/apps/$STAGE/$APP/\"  && rm -f ../../dist/$STAGE/$APP/*.* && mv ../../dist/$STAGE/$APP/browser/* ../../dist/$STAGE/$APP",
    "watch": "ng build --watch --configuration development",
    "test": "cd projects/web && ../../node_modules/.bin/vitest && cd ../web-plugin && ../../node_modules/.bin/vitest"
  },
  "publishConfig": {
    "directory": "dist/xt-plugin-web",
    "linkDirectory": true
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^20.3.9",
    "@angular/common": "^20.3.9",
    "@angular/compiler": "^20.3.9",
    "@angular/core": "^20.3.9",
    "@angular/forms": "^20.3.9",
    "@angular/platform-browser": "^20.3.9",
    "@angular/platform-browser-dynamic": "^20.3.9",
    "@angular/router": "^20.3.9",
    "@angular/cdk": "^20.2.4",
    "xt-components": "workspace:^",
    "xt-type": "workspace:^",
    "xt-store": "workspace:^",
    "@ngrx/signals": "^20.1.0",
    "rxjs": "^7.8.2",
    "primeng": "^20.3.0",
    "primeicons": "^7.0.0",
    "@primeuix/themes": "^1.2.5",
    "tslib": "^2.8.1",
    "tailwindcss": "~4.1.16",
    "es-module-shims": "^2.6.2",
    "@angular-architects/native-federation": "^20.1.7",
    "@softarc/native-federation-runtime": "^3.3.6"
  },
  "devDependencies": {
    "@primeuix/utils": "^0.6.1",
    "@primeuix/styled": "^0.7.2",
    "@angular-devkit/build-angular": "^20.3.8",
    "@angular/cli": "^20.3.8",
    "@angular/build": "^20.3.8",
    "@angular/compiler-cli": "^20.3.9",
    "ng-packagr": "^20.3.0",
    "typescript": "^5.9.3",
    "@analogjs/vite-plugin-angular":"^1.22.4",
    "@analogjs/vitest-angular": "^1.22.4",
    "jsdom": "^26.1.0",
    "vite": "^7.1.12",
    "vitest": "^3.2.4",
    "vite-tsconfig-paths": "~5.1.4",
    "happy-dom": "^18.0.1",
    "@tailwindcss/postcss": "~4.1.16",
    "postcss": "~8.5.6"
  }
}

